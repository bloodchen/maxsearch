{
  "headers": [
    {
      "source": "/s/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=604800, must-revalidate"
        }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
